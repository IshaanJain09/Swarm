<?xml version="1.0" encoding="utf-8"?>
<mujoco>
  <compiler autolimits="true"/>
  
  <option temperature="20"/>
  
  <asset>
    <model name="car" file="SARcar.xml"/>
    <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="512" height="512"/>
    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>
  
  <worldbody>
    <geom name="floor" size="100 100 .0001" type="plane" material="grid" condim="3"/>
    
    <body name="victim1" pos="2.5 2.5 0.4">
      <geom name="victim1_body" type="box" size="0.3 0.3 0.4" rgba="0.9 0.3 0.3 1" heatcapacity="3470" fluidshape="ellipsoid"/>
      <site name="victim1_temp" type="sphere" size="0.01" rgba="1 0 0 0.5"/>
    </body>
    
    <body name="victim2" pos="-3.0 1.5 0.35">
      <geom name="victim2_body" type="box" size="0.25 0.4 0.35" rgba="0.9 0.3 0.3 1" heatcapacity="3470" fluidshape="ellipsoid"/>
      <site name="victim2_temp" type="sphere" size="0.01" rgba="1 0 0 0.5"/>
    </body>
    
    <body name="victim3" pos="1.5 -2.8 0.45">
      <geom name="victim3_body" type="box" size="0.35 0.25 0.45" rgba="0.9 0.3 0.3 1" heatcapacity="3470" fluidshape="ellipsoid"/>
      <site name="victim3_temp" type="sphere" size="0.01" rgba="1 0 0 0.5"/>
    </body>
    
    <body name="victim4" pos="-1.8 -1.2 0.3">
      <geom name="victim4_body" type="box" size="0.28 0.28 0.3" rgba="0.9 0.3 0.3 1" heatcapacity="3470" fluidshape="ellipsoid"/>
      <site name="victim4_temp" type="sphere" size="0.01" rgba="1 0 0 0.5"/>
    </body>
    
    <body name="victim5" pos="3.5 -1.0 0.38">
      <geom name="victim5_body" type="box" size="0.32 0.35 0.38" rgba="0.9 0.3 0.3 1" heatcapacity="3470" fluidshape="ellipsoid"/>
      <site name="victim5_temp" type="sphere" size="0.01" rgba="1 0 0 0.5"/>
    </body>
    
    <geom name="rubble1" type="box" size="0.4 0.4 0.6" pos="-2.5 3.0 0.6" rgba="0.5 0.5 0.5 1"/>
    <geom name="rubble2" type="box" size="0.35 0.5 0.5" pos="4.0 0.5 0.5" rgba="0.5 0.5 0.5 1"/>
    <geom name="rubble3" type="box" size="0.5 0.3 0.4" pos="0.0 -3.5 0.4" rgba="0.5 0.5 0.5 1"/>
    
    <frame pos="0 0 -.095">
      <attach model="car" prefix="1"/>
    </frame>
    <frame pos="1.5 0 -.095">
      <attach model="car" prefix="2"/>
    </frame>
    <frame pos="-1.5 0 -.095">
      <attach model="car" prefix="3"/>
    </frame>
  </worldbody>
  
  <sensor>
    <touch name="victim1_temp_sensor" site="victim1_temp"/>
    <touch name="victim2_temp_sensor" site="victim2_temp"/>
    <touch name="victim3_temp_sensor" site="victim3_temp"/>
    <touch name="victim4_temp_sensor" site="victim4_temp"/>
    <touch name="victim5_temp_sensor" site="victim5_temp"/>
  </sensor>
</mujoco>